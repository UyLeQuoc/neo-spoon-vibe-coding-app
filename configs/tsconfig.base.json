{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "lib": ["ES2021"],
    "module": "ESNext",
    "noEmit": true,
    "strict": true,
    "composite": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "verbatimModuleSyntax": true,
    "moduleResolution": "bundler",
    "esModuleInterop": true,
    "target": "ES2022"
