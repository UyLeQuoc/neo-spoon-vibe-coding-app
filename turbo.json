{
  "$schema": "https://turborepo.org/schema.json",
  "globalEnv": ["NODE_ENV"],
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["build/**", "dist/**", "out/**"]
    },
    "build:watch": { "persistent": true, "cache": false },
    "dev": { "persistent": true, "cache": false, "with": ["build:watch"] },
    "test": { "cache": false },
    "deploy": { "dependsOn": ["build"] },
    "check:types": { "dependsOn": ["^check:types"] },
    "gen": {},
    "migrate": { "cache": false },
    "//#check": {},
    "//#check:fix": { "cache": false }
  }
}
